        </KeyFrame>
        <KeyFrame Cue="56%">
          <Setter Property="Opacity" Value="0.34" />
        </KeyFrame>
        <KeyFrame Cue="70%">
          <Setter Property="Opacity" Value="0.5" />
        </KeyFrame>
        <KeyFrame Cue="84%">
