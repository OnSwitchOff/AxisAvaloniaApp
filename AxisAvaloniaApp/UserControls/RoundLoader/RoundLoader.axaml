    </Style.Animations>
  </Style>

  <Style Selector="Ellipse.pos_el_7">
    <Style.Animations>
      <Animation Duration="0:0:0.5" IterationCount="INFINITE">
        <KeyFrame Cue="0%">
          <Setter Property="Opacity" Value="0" />
        </KeyFrame>
        <KeyFrame Cue="14%">
          <Setter Property="Opacity" Value="0.07" />
        </KeyFrame>
        <KeyFrame Cue="28%">
          <Setter Property="Opacity" Value="0.17" />
