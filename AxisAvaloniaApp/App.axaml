<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:AxisAvaloniaApp"
             x:Class="AxisAvaloniaApp.App">
    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>

    <Application.Styles>
      <StyleInclude Source="avares://AxisAvaloniaApp/UserControls/NavigationView/NavigationView.axaml"/>
      <StyleInclude Source="avares://AxisAvaloniaApp/UserControls/NavigationView/NavigationViewItem.axaml"/>
      <StyleInclude Source="avares://AxisAvaloniaApp/UserControls/RoundLoader/RoundLoader.axaml"/>
      <StyleInclude Source="avares://Avalonia.Controls.DataGrid/Themes/Default.xaml"/>
        <FluentTheme Mode="Light"/>
    </Application.Styles>
</Application>
