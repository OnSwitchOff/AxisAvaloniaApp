<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:settings="using:AxisAvaloniaApp.UserControls.Settings"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="AxisAvaloniaApp.Views.SettingsView">
  <Grid ColumnDefinitions="*,*,*,*">
	  <settings:ObjectSettings Grid.Column="0"/>
	  <settings:DocumentSettings Grid.Column="1"/>
	  <settings:DeviceSettings Grid.Column="2"/>
	  <settings:MainSettings Grid.Column="3"/>
  </Grid>
</UserControl>
